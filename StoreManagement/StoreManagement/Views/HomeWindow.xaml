                    Background="{x:Null}"
                    FontSize="22"
                    VerticalContentAlignment="Center"
                    HorizontalContentAlignment="Center"
                    FontWeight="SemiBold"
                    Cursor="Hand"
                    FocusVisualStyle="{x:Null}">
                </Button>
            </Grid>
        </Grid>
        <Grid
            x:Name="Main"
            Grid.Column="1">

            <Grid
                x:Name="nameApp"
                Height="100"
                Width="585"                
