                x:Name="grdBody_Store"
                Height="665"
                Width="1140"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Visibility="Hidden">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="Loaded">
                        <i:InvokeCommandAction Command="{Binding LoadStoreOnWindowCommand}" 
