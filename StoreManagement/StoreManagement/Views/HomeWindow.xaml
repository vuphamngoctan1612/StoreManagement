                        materialDesign:TextFieldAssist.TextFieldCornerRadius="24"
                        BorderThickness="1"
                        Margin="60,25,0,25">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="TextChanged">
                                <i:InvokeCommandAction Command="{Binding SearchAgencyCommand}" 
                                                       CommandParameter="{Binding ElementName=wdHome}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </TextBox>
                    <materialDesign:PackIcon
                        Kind="Search"
                        Height="30"
                        Width="30"
                        Foreground="#F79BB5"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="70,0"/>
