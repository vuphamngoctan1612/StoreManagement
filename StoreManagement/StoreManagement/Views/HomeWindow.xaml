            <!--Body main-->
            <Grid
                x:Name="grdBody_Main"
                DataContext="{StaticResource ReportVM}"
                Height="665"
                Width="1140"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Visibility="Visible">

                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="Loaded">
                        <i:InvokeCommandAction Command="{Binding LoadTop4AgencyCommand}" 
                                               CommandParameter="{Binding ElementName=wdHome}"/>
                    </i:EventTrigger>
                </i:Interaction.Triggers>

                <Grid.RowDefinitions>
                    <RowDefinition Height="70"></RowDefinition>
                    <RowDefinition Height="200"></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"></RowDefinition>
                        <RowDefinition Height="30"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" 
                        Margin="30,0,336.308,0">
                        <TextBlock
                            Text="MY STORES"
                            Foreground="#F97298"
                            FontSize="25"
                            HorizontalAlignment="Left"
                            FontWeight="SemiBold"
                            VerticalAlignment="Center">
                        </TextBlock>
                    </Grid>
                    <Grid Grid.Row="1" 
                        Margin="65,0,316.308,0.4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="54*"/>
                            <ColumnDefinition Width="65*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock
                            Text="TODAY RANKING"
                            FontSize="18"
                            Foreground="#504D85"
                            FontWeight="SemiBold"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Margin="20,3,0,3.4">
                        </TextBlock>
                    </Grid>
                </Grid>
                <Grid Grid.Row="1">
                    <WrapPanel  
                        x:Name="wpBody_Main_TopAgency"
                        Width="1140"
                        HorizontalAlignment="Left" 
                        ScrollViewer.VerticalScrollBarVisibility="Auto" 
                        ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
                        Margin="0">
                    </WrapPanel>
                </Grid>
                <Grid Grid.Row="2">
                    <lvc:CartesianChart 
                        Series="{Binding SeriesCollection}"
                        LegendLocation="Left"
                        Margin="0 20 25 0">
                        <lvc:CartesianChart.AxisX>
                            <lvc:Axis 
                                Title="Salesman" 
                                Labels="{Binding Labels}">
                            </lvc:Axis>
                        </lvc:CartesianChart.AxisX>
                        <lvc:CartesianChart.AxisY>
                            <lvc:Axis 
                                Title="" 
                                LabelFormatter="{Binding Formatter}">
                            </lvc:Axis>
                        </lvc:CartesianChart.AxisY>
                    </lvc:CartesianChart>
                </Grid>
            </Grid>
            <!--Body stores-->
            <Grid
