            <Grid
                x:Name="grdMenu_Logout"
                Height="60"
                Width="300"
                Margin="0,700,0,0">
                <materialDesign:PackIcon
                        x:Name="icnLogout"
                        Kind="ArrowRightBoldBoxOutline"
                        Foreground="#504D85"
                        Width="32" 
                        Height="32" 
                        VerticalAlignment="Center" 
                        Cursor="Hand" 
                        Margin="243,14,0,14" >
                </materialDesign:PackIcon>
                <Button
                    x:Name="btnLogout"
