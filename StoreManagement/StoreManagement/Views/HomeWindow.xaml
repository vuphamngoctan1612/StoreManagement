                            Height="50"
                            Background="{x:Null}"
                            BorderBrush="{x:Null}" 
                            Padding="0"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Right"
                            Margin="30"
                            Command="{Binding NextPageStoresCommand}"
                            CommandParameter="{Binding HomeWindow}">
                            <materialDesign:PackIcon 
                                Kind="ArrowRightBoldCircleOutline" 
                                Foreground="LightPink"
                                Height="50"
